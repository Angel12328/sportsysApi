#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules/mssql/bin/node_modules:/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules/mssql/node_modules:/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules:/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules/mssql/bin/node_modules:/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules/mssql/node_modules:/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules:/home/angel/Documentos/UNAH/I PAC 2026/bd1_I/proyecto/app/sportsysApi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules/mssql/bin/mssql" "$@"
else
  exec node  "$basedir/../.pnpm/mssql@12.2.0_@azure+core-client@1.10.1/node_modules/mssql/bin/mssql" "$@"
fi
